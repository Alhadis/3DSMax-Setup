	text=1 v=5000 // Original from GLSL Sandbox.\n\n#if ( defined(SFX_HLSL_5) || defined(SFX_HLSL_3) || defined(SFX_CGFX_3) )\n   #define mix lerp\n#endif\n\nstruct CustomCode1178Output \n{ \n\tfloat3 color; \n}; \n\nCustomCode1178Output CustomCode1178Func( float2 UV, float2 resolution, float time, int lines, int sublines, float brightness, float3 colorA, float3 colorB, float3 colorC ) \n{ \n    float x, y, xpos, ypos;\n    float t = time*30.0;\n    float3 c = float3(0,0,0);\n\n    #if defined(_3DSMAX_) && !defined(SFX_SWATCH)\n          resolution.y *= -1;\n   #endif\n\n    xpos = (UV.x / resolution.x/2.0);\n    ypos = (UV.y / (resolution.y/1.5));\n    \n    x = xpos;\n    for (float i = 0.0; i < lines; i += 1.0) {\n        for(float j = 0.0; j < sublines; j += 1.0){\n            y = ypos\n            + (0.30 * sin(x * 5.000 +( i * 1.5 + j) * 0.2 + t * 0.040)\n               + 0.300 * cos(x * 6.350 + (i  + j) * 0.2 + t * 0.050 * j)\n               + 0.024 * sin(x * 12.35 + ( i + j * 4.0 ) * 0.8 + t * 0.034 * (8.0 *  j))\n               + 0.5);\n            \n           float v = 1.0 - pow(clamp(abs(1.4 - y) * 5.0, 0.0,1.0), 0.25);\n            c += float3(v,v,v);\n        }\n    }\n    \n    c *= mix(\n             mix(colorC, colorA, ypos)\n             , mix(colorB, colorC, xpos)\n             ,(sin(t * 0.02) + 1.0) * 0.45\n             ) * brightness * clamp(t, 2.0, 2.5);\n    \n\tCustomCode1178Output OUT; \n\tOUT.color = c;\n\treturn OUT; \n} \n
