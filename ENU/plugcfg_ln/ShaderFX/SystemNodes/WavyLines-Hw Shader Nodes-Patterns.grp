	text=1 v=5000 color WavyLines( color UV, color resolution, int lines, int sublines, float brightness, color colorA, color colorB, color colorC )\n{\n\tfloat time = 0;\n    float x, y, xpos, ypos;\n    float t = time*30.0;\n    color c = color(0,0,0);\n    \n    xpos = (UV[0] / resolution[0]/2.0);\n    ypos = (UV[1] / (resolution[1]/1.5));\n    \n    x = xpos;\n    for (float i = 0.0; i < lines; i += 1.0) {\n        for(float j = 0.0; j < sublines; j += 1.0){\n            y = ypos\n            + (0.30 * sin(x * 5.000 +( i * 1.5 + j) * 0.2 + t * 0.040)\n               + 0.300 * cos(x * 6.350 + (i  + j) * 0.2 + t * 0.050 * j)\n               + 0.024 * sin(x * 12.35 + ( i + j * 4.0 ) * 0.8 + t * 0.034 * (8.0 *  j))\n               + 0.5);\n            \n           float v = 1.0 - pow(clamp(abs(1.4 - y) * 5.0, 0.0,1.0), 0.25);\n            c += color(v,v,v);\n        }\n    }\n    \n    c *= mix(\n             mix(colorC, colorA, ypos)\n             , mix(colorB, colorC, xpos)\n             ,(sin(t * 0.02) + 1.0) * 0.45\n             ) * brightness * clamp(t, 2.0, 2.5);\n\n    return Gamma(c);\n}\n\nWavyLines([[ INPUT = UV ? color(u,-v+1,0) ]], [[ INPUT = Resolution ? color([[PROP=valueX_Resolution]], [[PROP=valueY_Resolution]],0) ]], [[ INPUT = Lines ? [[PROP=value_Lines]] ]], [[ INPUT = SubLines ? [[PROP=value_SubLines]] ]], [[ INPUT = Brightness ? [[PROP=value_Brightness]] ]], [[ INPUT = ColorA ? color([[PROP=color_ColorA]]) ]], [[ INPUT = ColorB ? color([[PROP=color_ColorB]]) ]], [[ INPUT = ColorC ?color( [[PROP=color_ColorC]]) ]])
